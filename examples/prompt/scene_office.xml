<mujoco model="openai_busy_office">
  <compiler angle="degree" coordinate="local" inertiafromgeom="true"/>
  <option timestep="0.002" gravity="0 0 -9.81" integrator="RK4"/>
  <size njmax="2000" nconmax="400"/>

  <asset>
    <!-- No textures/materials per instructions -->
  </asset>

  <default>
    <joint damping="1" armature="0.01" limited="false"/>
    <geom contype="1" conaffinity="1" condim="4" friction="0.9 0.1 0.01" density="500" margin="0.002" rgba="0.7 0.7 0.7 1"/>
  </default>

  <worldbody>
    <!-- Floor -->
    <geom name="floor" type="plane" pos="0 0 0" size="20 20 0.1" rgba="0.92 0.92 0.92 1" friction="1 0.2 0.02"/>

    <!-- Back wall -->
    <body name="wall_back" pos="0 -6 1.5">
      <geom name="wall_back_geom" type="box" pos="0 0 0" size="8 0.1 1.5" rgba="0.95 0.95 0.97 1" contype="0" conaffinity="0"/>
    </body>

    <!-- Side wall -->
    <body name="wall_side" pos="6 0 1.5">
      <geom name="wall_side_geom" type="box" pos="0 0 0" size="0.1 8 1.5" rgba="0.95 0.95 0.97 1" contype="0" conaffinity="0"/>
    </body>

    <!-- Ceiling light panel (visual only) -->
    <body name="ceiling_light" pos="0 0 3.0">
      <geom name="ceiling_panel" type="box" pos="0 0 0" size="2.5 1.2 0.03" rgba="1 1 0.92 1" contype="0" conaffinity="0"/>
    </body>

    <!-- Large rug -->
    <body name="rug" pos="0.5 -0.5 0.005">
      <geom name="rug_geom" type="box" pos="0 0 0" size="3.5 2.5 0.005" rgba="0.15 0.18 0.22 1" friction="1.2 0.3 0.03"/>
    </body>

    <!-- Shared desk 1 -->
    <body name="desk1" pos="-2.0 -1.0 0">
      <geom name="desk1_top" type="box" pos="0 0 0.76" size="1.2 0.6 0.03" rgba="0.62 0.45 0.28 1" contype="0" conaffinity="0"/>
      <geom name="desk1_leg1" type="cylinder" pos="1.1 0.5 0.38" size="0.03 0.38" rgba="0.18 0.18 0.2 1" contype="0" conaffinity="0"/>
      <geom name="desk1_leg2" type="cylinder" pos="-1.1 0.5 0.38" size="0.03 0.38" rgba="0.18 0.18 0.2 1" contype="0" conaffinity="0"/>
      <geom name="desk1_leg3" type="cylinder" pos="1.1 -0.5 0.38" size="0.03 0.38" rgba="0.18 0.18 0.2 1" contype="0" conaffinity="0"/>
      <geom name="desk1_leg4" type="cylinder" pos="-1.1 -0.5 0.38" size="0.03 0.38" rgba="0.18 0.18 0.2 1" contype="0" conaffinity="0"/>
    </body>

    <!-- Shared desk 2 -->
    <body name="desk2" pos="2.2 -1.0 0">
      <geom name="desk2_top" type="box" pos="0 0 0.76" size="1.2 0.6 0.03" rgba="0.62 0.45 0.28 1" contype="0" conaffinity="0"/>
      <geom name="desk2_leg1" type="cylinder" pos="1.1 0.5 0.38" size="0.03 0.38" rgba="0.18 0.18 0.2 1" contype="0" conaffinity="0"/>
      <geom name="desk2_leg2" type="cylinder" pos="-1.1 0.5 0.38" size="0.03 0.38" rgba="0.18 0.18 0.2 1" contype="0" conaffinity="0"/>
      <geom name="desk2_leg3" type="cylinder" pos="1.1 -0.5 0.38" size="0.03 0.38" rgba="0.18 0.18 0.2 1" contype="0" conaffinity="0"/>
      <geom name="desk2_leg4" type="cylinder" pos="-1.1 -0.5 0.38" size="0.03 0.38" rgba="0.18 0.18 0.2 1" contype="0" conaffinity="0"/>
    </body>

    <!-- Monitors (static props on desks; visual only) -->
    <body name="monitor1" pos="-2.0 -1.15 0.79">
      <geom name="monitor1_screen" type="box" pos="0 0 0.18" size="0.30 0.03 0.18" rgba="0.05 0.05 0.06 1" contype="0" conaffinity="0"/>
      <geom name="monitor1_bezel" type="box" pos="0 0 0.18" size="0.32 0.04 0.20" rgba="0.1 0.1 0.12 1" contype="0" conaffinity="0"/>
      <geom name="monitor1_stand" type="box" pos="0 0 0.05" size="0.08 0.05 0.05" rgba="0.12 0.12 0.14 1" contype="0" conaffinity="0"/>
    </body>

    <body name="monitor2" pos="2.2 -1.15 0.79">
      <geom name="monitor2_screen" type="box" pos="0 0 0.18" size="0.30 0.03 0.18" rgba="0.05 0.05 0.06 1" contype="0" conaffinity="0"/>
      <geom name="monitor2_bezel" type="box" pos="0 0 0.18" size="0.32 0.04 0.20" rgba="0.1 0.1 0.12 1" contype="0" conaffinity="0"/>
      <geom name="monitor2_stand" type="box" pos="0 0 0.05" size="0.08 0.05 0.05" rgba="0.12 0.12 0.14 1" contype="0" conaffinity="0"/>
    </body>

    <!-- Whiteboard -->
    <body name="whiteboard" pos="-3.5 -5.85 1.4">
      <geom name="whiteboard_panel" type="box" pos="0 0 0" size="1.6 0.03 0.9" rgba="0.98 0.98 0.99 1" contype="0" conaffinity="0"/>
      <geom name="whiteboard_frame" type="box" pos="0 0 0" size="1.65 0.04 0.95" rgba="0.75 0.75 0.78 1" contype="0" conaffinity="0"/>
    </body>

    <!-- Couch -->
    <body name="couch" pos="-3.0 2.3 0">
      <geom name="couch_base" type="box" pos="0 0 0.25" size="1.2 0.5 0.25" rgba="0.25 0.28 0.32 1" contype="0" conaffinity="0"/>
      <geom name="couch_back" type="box" pos="0 -0.45 0.65" size="1.2 0.08 0.45" rgba="0.23 0.26 0.30 1" contype="0" conaffinity="0"/>
      <geom name="couch_arm1" type="box" pos="1.12 0 0.55" size="0.08 0.5 0.35" rgba="0.23 0.26 0.30 1" contype="0" conaffinity="0"/>
      <geom name="couch_arm2" type="box" pos="-1.12 0 0.55" size="0.08 0.5 0.35" rgba="0.23 0.26 0.30 1" contype="0" conaffinity="0"/>
    </body>

    <!-- Plant (static pot + trunk/leaves visual) -->
    <body name="plant" pos="4.8 4.6 0">
      <geom name="plant_pot" type="cylinder" pos="0 0 0.18" size="0.22 0.18" rgba="0.45 0.25 0.18 1" contype="0" conaffinity="0"/>
      <geom name="plant_trunk" type="cylinder" pos="0 0 0.55" size="0.04 0.35" rgba="0.35 0.22 0.12 1" contype="0" conaffinity="0"/>
      <geom name="plant_canopy" type="sphere" pos="0 0 0.95" size="0.28" rgba="0.12 0.35 0.18 1" contype="0" conaffinity="0"/>
      <geom name="plant_canopy2" type="sphere" pos="0.18 0.05 0.85" size="0.22" rgba="0.10 0.32 0.16 1" contype="0" conaffinity="0"/>
      <geom name="plant_canopy3" type="sphere" pos="-0.15 -0.08 0.82" size="0.20" rgba="0.10 0.30 0.15 1" contype="0" conaffinity="0"/>
    </body>

    <!-- Rolling chairs (static, but could be made dynamic; keeping static for stability) -->
    <body name="chair1" pos="-1.2 -0.2 0">
      <geom name="chair1_seat" type="cylinder" pos="0 0 0.48" size="0.22 0.05" rgba="0.2 0.2 0.22 1" contype="0" conaffinity="0"/>
      <geom name="chair1_back" type="box" pos="0 -0.18 0.70" size="0.22 0.05 0.20" rgba="0.18 0.18 0.2 1" contype="0" conaffinity="0"/>
      <geom name="chair1_pole" type="cylinder" pos="0 0 0.25" size="0.04 0.25" rgba="0.12 0.12 0.14 1" contype="0" conaffinity="0"/>
      <geom name="chair1_base1" type="capsule" pos="0 0 0.05" fromto="0 0 0.05 0.35 0 0.05" size="0.03" rgba="0.12 0.12 0.14 1" contype="0" conaffinity="0"/>
      <geom name="chair1_base2" type="capsule" pos="0 0 0.05" fromto="0 0 0.05 -0.35 0 0.05" size="0.03" rgba="0.12 0.12 0.14 1" contype="0" conaffinity="0"/>
      <geom name="chair1_base3" type="capsule" pos="0 0 0.05" fromto="0 0 0.05 0 0.35 0.05" size="0.03" rgba="0.12 0.12 0.14 1" contype="0" conaffinity="0"/>
      <geom name="chair1_base4" type="capsule" pos="0 0 0.05" fromto="0 0 0.05 0 -0.35 0.05" size="0.03" rgba="0.12 0.12 0.14 1" contype="0" conaffinity="0"/>
    </body>

    <body name="chair2" pos="3.0 -0.2 0">
      <geom name="chair2_seat" type="cylinder" pos="0 0 0.48" size="0.22 0.05" rgba="0.2 0.2 0.22 1" contype="0" conaffinity="0"/>
      <geom name="chair2_back" type="box" pos="0 -0.18 0.70" size="0.22 0.05 0.20" rgba="0.18 0.18 0.2 1" contype="0" conaffinity="0"/>
      <geom name="chair2_pole" type="cylinder" pos="0 0 0.25" size="0.04 0.25" rgba="0.12 0.12 0.14 1" contype="0" conaffinity="0"/>
      <geom name="chair2_base1" type="capsule" pos="0 0 0.05" fromto="0 0 0.05 0.35 0 0.05" size="0.03" rgba="0.12 0.12 0.14 1" contype="0" conaffinity="0"/>
      <geom name="chair2_base2" type="capsule" pos="0 0 0.05" fromto="0 0 0.05 -0.35 0 0.05" size="0.03" rgba="0.12 0.12 0.14 1" contype="0" conaffinity="0"/>
      <geom name="chair2_base3" type="capsule" pos="0 0 0.05" fromto="0 0 0.05 0 0.35 0.05" size="0.03" rgba="0.12 0.12 0.14 1" contype="0" conaffinity="0"/>
      <geom name="chair2_base4" type="capsule" pos="0 0 0.05" fromto="0 0 0.05 0 -0.35 0.05" size="0.03" rgba="0.12 0.12 0.14 1" contype="0" conaffinity="0"/>
    </body>

    <!-- Moving props (with freejoint) -->
    <body name="coffee_mug" pos="-1.8 -0.95 0.82">
      <freejoint name="coffee_mug_free"/>
      <geom name="mug_body" type="cylinder" pos="0 0 0.05" size="0.045 0.05" rgba="0.95 0.25 0.20 1" density="300"/>
      <geom name="mug_handle" type="capsule" fromto="0.045 0 0.05 0.085 0 0.05" size="0.012" rgba="0.95 0.25 0.20 1" density="300"/>
    </body>

    <body name="laptop" pos="2.05 -1.0 0.80" quat="1 0 0 0">
      <freejoint name="laptop_free"/>
      <geom name="laptop_base" type="box" pos="0 0 0.01" size="0.16 0.11 0.01" rgba="0.2 0.2 0.22 1" density="400"/>
      <geom name="laptop_screen" type="box" pos="-0.16 0 0.13" size="0.01 0.11 0.12" rgba="0.08 0.08 0.09 1" density="200"/>
    </body>

    <body name="paper_stack" pos="0.2 -0.8 0.78">
      <freejoint name="paper_stack_free"/>
      <geom name="paper_stack_geom" type="box" pos="0 0 0.01" size="0.10 0.07 0.01" rgba="0.98 0.98 0.98 1" density="200"/>
    </body>

    <!-- Trash bin (static) -->
    <body name="trash_bin" pos="4.4 -3.8 0">
      <geom name="bin_outer" type="cylinder" pos="0 0 0.25" size="0.20 0.25" rgba="0.25 0.27 0.30 1" contype="0" conaffinity="0"/>
    </body>

    <!-- Small rolling robot cart (moving) -->
    <body name="robot_cart" pos="0.5 1.6 0.10">
      <freejoint name="robot_cart_free"/>
      <geom name="cart_base" type="box" pos="0 0 0.10" size="0.25 0.18 0.10" rgba="0.35 0.36 0.38 1" density="600"/>
      <geom name="cart_top" type="box" pos="0 0 0.24" size="0.23 0.16 0.02" rgba="0.55 0.56 0.58 1" density="300"/>
      <geom name="cart_bumper" type="capsule" fromto="-0.25 -0.18 0.08 0.25 -0.18 0.08" size="0.03" rgba="0.20 0.20 0.22 1" density="200"/>
      <geom name="cart_bumper2" type="capsule" fromto="-0.25 0.18 0.08 0.25 0.18 0.08" size="0.03" rgba="0.20 0.20 0.22 1" density="200"/>
    </body>

    <!-- A few moving boxes to imply activity -->
    <body name="shipping_box1" pos="1.0 3.0 0.15">
      <freejoint name="shipping_box1_free"/>
      <geom name="shipping_box1_geom" type="box" pos="0 0 0.15" size="0.20 0.16 0.15" rgba="0.72 0.56 0.34 1" density="250"/>
    </body>

    <body name="shipping_box2" pos="-0.2 3.4 0.10">
      <freejoint name="shipping_box2_free"/>
      <geom name="shipping_box2_geom" type="box" pos="0 0 0.10" size="0.16 0.14 0.10" rgba="0.70 0.54 0.32 1" density="250"/>
    </body>

    <!-- Camera -->
    <camera name="main_cam" pos="0 8 3.2" quat="0.9659 0.2588 0 0" fovy="55"/>
  </worldbody>

  <actuator>
    <!-- No actuators; scene-only -->
  </actuator>
</mujoco>